<%- include("../admin/header.ejs") %> <%- include("../admin/sidebar.ejs") %>
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <div class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1 class="m-0">View Complaint</h1>
        </div>
        <!-- /.col -->
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-d"><a href="/">Home</a></li>
            <li class="breadcrumb-d active">Display</li>
          </ol>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </div>
  <!-- /.content-header -->
  <!-- </div> -->
  <div class="card">
    <!-- /.card-header -->
    <div class="card-body">
      <table id="example1" class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>Id</th>
            <th>Complaint Title</th>
            <th>Semester </th>
            <th>Roll Number </th>
            <th>Course</th>
            <th>Complaint info</th>
            <th>Image</th>
          </tr>
        </thead>
        <tfoot>
          <tbody>
            <td><%=d._id%></td>
              <td><%=d.comptitle%></td>
              <td><%=d.semester%></td>
              <td><%=d.rollnumber%></td>
              <td><%=d.course%></td>
              <td><%=d.complaint%></td>
              <th><img src="<%= d.image.url %>" width="50px" alt="" /></th>
        </tbody>
        </tfoot>
      </table>
    </div>
    <!-- /.card-body -->
  </div>
  <!-- /.card -->
</div>
<%- include("../admin/footer.ejs")%>
